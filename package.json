  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "Firefox ESR",
      "IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
