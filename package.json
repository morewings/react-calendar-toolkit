{
  "name": "react-calendar-toolkit",
  "version": "0.3.7",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "description": "React Calendar Toolkit is a set of React Components capable of rendering various calendars, datepickers etc.",
